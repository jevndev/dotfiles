profile undocked {
	output eDP-1 enable scale 1
	output eDP-1 mode 2880x1920@120Hz position 0,0
}

profile docked {
	output eDP-1 disable
	output DP-5 mode 3440x1440@84.957Hz position 0,0
}

profile disabling_eDP1_in_docked_makes_undocked_profile_never_match_so_this_reenables_eDP1_when_nothing_is_matched {
	output * enable
	exec swaymsg output eDP-1 enable
}
